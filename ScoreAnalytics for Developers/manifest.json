{
	"manifest_version": 2,
	"name": "Score Analytics",
	"description": "Аналитика скора в app RSSchool",
	"version": "1.1.1",
	"icons": {"128": "icon.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"cookies",
		"*://github.com/"
	],
	"content_security_policy": "script-src 'self' https://cdn.jsdelivr.net; object-src 'self'"
}